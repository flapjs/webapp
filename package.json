    "lint": "npm run lint-js && npm run lint-css",
    "lint-js": "eslint \"src/\" --ext \".js\" --ext \".jsx\"",
    "lint-css": "stylelint \"src/**/*.css\"",
    "storybook": "start-storybook",
    "setup-vscode": "node tools/vscode/setup.js"
  },
